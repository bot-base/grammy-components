{
  "compilerOptions": {
    "moduleResolution": "node16",
    "module": "ESNext",
    "outDir": "./dist",
    "target": "ES2019",
    "lib": [
      "ES2019"
    ],
    "declaration": true,
    "strict": false,
    "esModuleInterop": true,
    "inlineSourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "skipLibCheck": true
  },
  "include": [
    "src/"
  ]
}
